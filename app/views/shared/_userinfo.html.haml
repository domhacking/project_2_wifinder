%h1 
  - if current_user
    User Info
    %p
      %b Image
      .circular
        = link_to image_tag(current_user.profile_image), current_user.profile_image
      %p
        %b User Name:
        = current_user.name

      %p
        %b Email:
        = current_user.email
  
    .btn
      = link_to 'FIND ME A CAFE', cafes_path
    .btn  
      = link_to 'EDIT MY PROFILE', edit_user_registration_path
      / = link_to 'MY FAVOURITES', _path
         


