- content_for :left do
  %h1 
    - if current_user
      User Info
      %p
        %b Image
        .circular
          = link_to image_tag(current_user.profile_image.thumb.url), current_user.profile_image.detail.url
        %p
          %b User Name:
          = current_user.name

        %p
          %b Email:
          = current_user.email
        %p
          %b Favourites:

- content_for :center do
    = render 'shared/map'

          
          
          



